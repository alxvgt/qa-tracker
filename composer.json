{
    "name": "alxvng/qatracker",
    "description": "QATracker is a tool to collect, store and render quality assurance indicators.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Alxvng\\QATracker\\": "src/",
            "Alxvng\\QATracker\\Tests\\": "tests/"
        },
        "files": [
            "Root.php"
        ]
    },
    "support": {
        "issues": "https://github.com/alxvgt/qatracker/issues"
    },
    "bin": [
        "bin/qatracker"
    ],
    "require": {
        "php": ">=7.4",
        "ext-xml": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-fileinfo": "*",
        "symfony/console": "^7.0",
        "goat1000/svggraph": "^3.5",
        "twig/twig": "^3.0",
        "symfony/string": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/finder": "^7.0",
        "flow/jsonpath": "^0.5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^7.0"
    },
    "config": {
        "sort-packages": true
    }
}
